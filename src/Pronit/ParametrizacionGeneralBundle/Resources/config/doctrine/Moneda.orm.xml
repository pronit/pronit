<?xml version="1.0" encoding="UTF-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                    http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Pronit\ParametrizacionGeneralBundle\Entity\Moneda" table="pgener_monedas">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO" />
        </id>
        <field name="nombre" column="nombre" type="string" length="100" />
        <field name="abreviatura" type="string" length="20" />
        <field name="signoMonetario" type="string" length="5" />
        <field name="codigoISO" type="string" length="3" />
    </entity>
</doctrine-mapping>