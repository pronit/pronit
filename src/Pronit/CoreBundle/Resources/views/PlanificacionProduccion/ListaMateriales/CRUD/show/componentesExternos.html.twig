<td>
{% block field %}   
    <table class="table table-bordered">
        <thead>
            <th>Material</th>
            <th>Cantidad</th>
        </thead>
        <tbody>
            {% for item in value %}
                
            <tr>
                <td>{{ item.material }}</td>
                <td>{{ item.cantidad }}</td>
            </tr>
            {% endfor %}   
            
        </tbody>
    </table>
{% endblock %}
</td>
