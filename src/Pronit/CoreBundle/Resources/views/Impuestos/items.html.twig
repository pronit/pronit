<td>
{% block field %}   
    <table class="table table-bordered">
        <thead>
            <th>Operación Contable</th>
            <th>Alícuota</th>
        </thead>
        <tbody>
            {% for item in value %}
            <tr>
                <td>{{ item.operacionContable }}</td>
                <td>{{ item.alicuota }}</td>
            </tr>
            {% endfor %}            
        </tbody>
    </table>
{% endblock %}
</td>
